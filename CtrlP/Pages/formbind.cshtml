@page 
@model CtrlP.Pages.FormbindModel
@{
    ViewData["Title"] = Model.Page_Title;
}
<h2>Editar o Medidor: </h2>  
<hr />  
<div class="row">  
    <form method="post">
        <input asp-for="Order.HWID" /><br />
        <input asp-for="Order.HWIP" /><br />
        <input asp-for="Order.ServersIP" /><br />
        <input asp-for="Order.OperationType" /><br />
        <input asp-for="Order.State" /><br />
         @for (int i = 0; i < Model.Order.CalibrationParameters.Count(); i++)
        {
           <label asp-for="Order.CalibrationParameters[i]" >valor:</label><br>
           <input asp-for="Order.CalibrationParameters[i]" /><br>
        }
        <input type="submit"/>
    </form>
</div>  
<div>  
    <a asp-page="../medidores">Voltar a Lista</a>  
</div>  
@section Scripts {  
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}  
} 